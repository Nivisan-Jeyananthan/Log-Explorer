[Application]
name=org.logexplorer.LogExplorer
runtime=org.gnome.Platform/x86_64/44
sdk=org.gnome.Sdk/x86_64/44
command=log-explorer

[Context]
shared=network;ipc;
sockets=x11;wayland;
devices=dri;

[Session Bus Policy]
org.freedesktop.Notifications=talk

[Extension org.logexplorer.LogExplorer.Debug]
directory=lib/debug
autodelete=true
no-autodownload=true

[Build]
built-extensions=org.logexplorer.LogExplorer.Debug;